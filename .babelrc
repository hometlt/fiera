{
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-runtime",
    ["@babel/plugin-proposal-class-properties", { "loose": true }]
  ],
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "false",
        "useBuiltIns": "usage",
        "targets": "> 0.25%, not dead",
        "corejs": 3
      }
    ]
  ]
}
